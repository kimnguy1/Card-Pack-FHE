## Copy to .env and fill values before deploying to Sepolia
# Do NOT commit your real .env to git. Keep secrets private.

# Twelve-word mnemonic (deployer account) or private key (use with caution)
MNEMONIC="your twelve word mnemonic here"

# INFURA project id (or any Sepolia RPC provider key)
INFURA_API_KEY="your_infura_project_id"

# Optional: full RPC URL to Sepolia (overrides INFURA_API_KEY if set)
SEPOLIA_RPC_URL="https://sepolia.infura.io/v3/your_infura_project_id"

# Etherscan API key for contract verification
ETHERSCAN_API_KEY="your_etherscan_api_key"
